<% if (item.photos && item.photos.length) { %>
<div class="cube-gallery">
    <% item.photos.forEach(function(photo, i) { %>
    <a class="gallery-item" href="<%- url_for(`images/${photo}`) %>" rel="gallery_<%= item._id %>">
        <img src="<%- url_for(`images/${photo}`) %>" itemprop="image" />
    </a>
    <% }) %>
</div>
<% } %>